#include <iostream>

using namespace std;

int arr[501][501];

int main()
{
    int n;
    cin >> n;
    for(int i = 0;i < n;++i)
    {
        int x1, y1, x2, y2;
        cin >> x1 >> y1 >> x2 >> y2;
        for(int y = y1;y < y2;++y)
        {
            for(int x = x1;x < x2;++x) arr[y][x] = 1;
        }
    }

    int area = 0;
    
    for(int i = 0;i <= 500;++i)
    {
        for(int j = 0;j <= 500;++j)
        {
            if(arr[i][j] == 1) area += 1;
        }
    }

    cout << area << endl;

    return 0;
}
