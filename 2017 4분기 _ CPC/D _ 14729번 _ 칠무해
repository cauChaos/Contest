#include <stdio.h>

double seven[7]={101.0,101.0,101.0,101.0,101.0,101.0,101.0};

int main()
{
    int N;
    scanf("%d",&N);
    for(int i=0;i<N;i++)
    {
        double D;
        scanf("%lf",&D);
        for(int j=0;j<7;j++)
        {
            if(D<seven[j])
            {
                double temp=D;
                D=seven[j];
                seven[j]=temp;
            }
        }
    }
    for(int i=0;i<7;i++) printf("%.3lf\n",seven[i]);
    return 0;
}
